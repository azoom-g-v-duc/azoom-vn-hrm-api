  "scripts": {
    "build:openapi": "rm -rf openapi.json && multi-file-swagger openapi.yaml > openapi.json",
    "build:doc": "redoc-cli bundle ./openapi.json -o openapi.html",
    "serve": "yarn build:openapi && redoc-cli serve ./openapi.json"
  },
  "devDependencies": {
    "multi-file-swagger": "^2.3.0",
    "redoc": "^2.0.0-rc.18",
    "redoc-cli": "^0.9.2"
  },
  "dependencies": {
    "firebase-admin": "^8.10.0"
  }
}
